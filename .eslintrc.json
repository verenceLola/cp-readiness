{
    "extends": "airbnb",
    "env": {
        "node": true,
        "es6": true,
        "browser": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js","jsx"]}],
        "react/prefer-stateless-function": "off",
        "no-param-reassign":"off"
        },
    "parser": "babel-eslint"
}
